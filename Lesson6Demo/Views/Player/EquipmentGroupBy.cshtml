@{
}

@model IEnumerable<IGrouping<EquipmentType, Equipment>>

    @foreach (var group in Model)
    {
        <h1>
            @group.Key.ToString()
        </h1>

        <ul>
            @foreach (var element in group)
            {
                <li>
                    @element.Name
                </li>
            }
        </ul>
    }