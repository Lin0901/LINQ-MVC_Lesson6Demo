@{
}

@model IEnumerable<IGrouping<Region, PlayerCharacter>>

@foreach (var region in Model)
{
    <h1>@region.Key.ToString()</h1>

    <ul>
        @foreach (var player in region)
        {
            <li>
                @player.Name
            </li>
        }
    </ul>
}