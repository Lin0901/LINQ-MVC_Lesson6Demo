@{
}

@model IEnumerable<IGrouping<bool, Equipment>>

    @foreach (var group in Model)
{
    <h1>
        @if (group.Key)
        {
            <span>
                Very Strong
            </span>
        } else
        {
            <span>
                Not Strong
            </span>
        }
    </h1>

        <ul>
            @foreach (var element in group)
            {
                <li>
                    @element.Name (Strength Mod @element.StrengthModifier)
                </li>
            }
        </ul>
    }